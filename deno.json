{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "run": "DENO_DEPLOYMENT_ID=$(git rev-parse --short HEAD) deno run -A main.ts"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": {
    "options": {
      "semiColons": false
    }
  },
  "lock": false
}
